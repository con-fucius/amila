# LLM Provider Configuration
# Choose between 'mistral' (default), 'gemini', 'bedrock', 'qwen', or 'openrouter'
QUERY_LLM_PROVIDER=mistral
QUERY_LLM_MODEL=devstral-small-latest

# Mistral AI Configuration
# Get your API key from: https://console.mistral.ai/
MISTRAL_API_KEY=your_mistral_api_key_here
MISTRAL_MODEL=devstral-small-latest

# Graphiti (Knowledge Graph) AI Setup
GRAPHITI_LLM_PROVIDER=mistral
GRAPHITI_LLM_MODEL=devstral-small-latest
GRAPHITI_EMBEDDING_PROVIDER=local
GRAPHITI_EMBEDDING_MODEL=all-MiniLM-L6-v2
GRAPHITI_EMBEDDING_DIMENSIONS=384

# Google Gemini (Optional)
GOOGLE_API_KEY=your_google_api_key_here

# Oracle Database Configuration
ORACLE_HOST=localhost
ORACLE_PORT=1522
ORACLE_SERVICE_NAME=FREEPDB1
ORACLE_USERNAME=system
ORACLE_PASSWORD=password
ORACLE_DEFAULT_CONNECTION=TestUserCSV

# Apache Doris Configuration
DORIS_MCP_ENABLED=true
DORIS_DB_HOST=localhost
DORIS_DB_PORT=9030
DORIS_DB_USER=root
DORIS_DB_PASSWORD=
DORIS_DB_DATABASE=demo

# PostgreSQL Configuration
POSTGRES_ENABLED=false
POSTGRES_HOST=localhost
POSTGRES_PORT=5432
POSTGRES_DATABASE=postgres
POSTGRES_USER=postgres
POSTGRES_PASSWORD=postgres
POSTGRES_READ_ONLY=true

# Redis Configuration
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=redispass123

# FalkorDB Configuration
FALKORDB_HOST=localhost
FALKORDB_PORT=6380
FALKORDB_DATABASE=amil_knowledge_graph

# Celery & Infrastructure
CELERY_BROKER_URL=redis://:redispass123@localhost:6379/0
CELERY_RESULT_BACKEND=redis://:redispass123@localhost:6379/1

# Application Security
# IMPORTANT: Generate a new random string (32 chars)
JWT_SECRET_KEY=your_jwt_secret_key_here
JWT_REFRESH_SECRET_KEY=your_jwt_refresh_secret_key_here

# Monitoring & Observability
ENVIRONMENT=development
LOG_LEVEL=INFO
OTEL_EXPORTER_OTLP_ENDPOINT=http://localhost:4318
LANGFUSE_ENABLED=false